BEGIN

UPDATE configuration
SET prop_value = 'true'
WHERE app_id = 'esignapi'
  AND prop_name = 'EXECUTE_PCOE_TEST'
  AND lob_id IN ('dna', 'dnaus');

UPDATE configuration
SET prop_value = '192.168.1.25:8080'   -- <- your new IP:PORT
WHERE app_id = 'esignapi'
  AND prop_name = 'PCOE_HOST'
  AND lob_id = 'default';

COMMIT;
END;
/